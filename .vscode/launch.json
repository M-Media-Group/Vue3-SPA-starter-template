    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js"
            ],
            "name": "Run dev",
            "preLaunchTask": "npm: dev",
            "url": "https://192.168.10.17:4173",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js"
            ],
            "name": "Test app",
            "preLaunchTask": "npm: test:e2e:dev",
            "url": "https://192.168.10.17:4173",
            "webRoot": "${workspaceFolder}"
        }
    ]
}